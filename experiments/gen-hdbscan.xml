<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project HDBSCAN">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="/home/toni/workspace-neon"/>
    <property name="dir.jarfile" value="${dir.buildfile}"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/HDBSCAN.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="ca.ualberta.cs.experiments.ExperimentHDBSCANStar"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="/home/toni/git/HDBSCAN_Star/bin"/>
        </jar>
    </target>
</project>
